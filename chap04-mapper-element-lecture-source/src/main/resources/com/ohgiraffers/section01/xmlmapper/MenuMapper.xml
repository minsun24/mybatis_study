<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ohgiraffers.section01.xmlmapper.ElementMapper">
    <resultMap id="menuResultMap1" type="com.ohgiraffers.section01.xmlmapper.MenuDTO">
        <id property="menuResultMap1" type="com.ohgiraffers.section01.xmlmapper.MenuDTO"/i>
        
    </resultMap>
    <select id="selectResultMapTest" resultMap="">
        SELECT
            A.MENU_CODE
            , A.MENU_NAME
            , A.MENU_PRICE
            , CATEGORY_STATUS
            , A.ORDERABLE_STATUS
        FROM TBL_MENU A
        WHERE A.ORDERABLE_STATUS = 'Y'
    </select>
</mapper>